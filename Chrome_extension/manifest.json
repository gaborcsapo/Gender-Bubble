{
  "name": "Gender bubble",
  "manifest_version": 2,
  "version": "0.1",
  "permissions": [
    "<all_urls>","webRequest","webRequestBlocking", "storage"
  ],
  "browser_action": {
      "default_icon": "favicon.png",
      "default_title": "That's the tool tip",
      "default_popup": "./popup.html"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"],
    "persistent": true
  }
}
